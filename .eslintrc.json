{
    "extends": "airbnb",
    "rules": {
        "no-multi-spaces": 1,
        "no-underscore-dangle": 0,
        "no-unused-vars": 1,
        "arrow-parens": 0,
        "keyword-spacing": 0,
        "object-curly-spacing": 0,
        "no-param-reassign": [1, {"props": false}],
        "key-spacing": 0,
        "space-before-function-paren": 0,
        "no-trailing-spaces": ["error", {"skipBlankLines": true}],
        "space-in-parens": 0,
        "space-unary-ops": 0,
        "no-console": 1,
        "arrow-body-style": ["error", "as-needed"],
        "import/no-unresolved": 0,
        "import/no-extraneous-dependencies": 0,
        "max-len": ["warn", 150],
        "no-shadow": ["error", {"allow": ["resolve", "reject", "done", "cb", "err"]}],
        "class-methods-use-this": 0,
        "consistent-return": 0,
        "new-cap": 0,
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1,
                "VariableDeclarator": 1,
                "outerIIFEBody": 1,
                // MemberExpression: null,
                // CallExpression: {
                // parameters: null,
                // },
                "FunctionDeclaration": {
                    "parameters": 1,
                    "body": 1
                },
                "FunctionExpression": {
                    "parameters": 1,
                    "body": 1
                }
            }
        ]
    }
}